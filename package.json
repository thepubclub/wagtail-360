{
  "name": "wagtail-360",
  "version": "1.0.0",
  "description": "This app provides an admin interface to choose google street view imagery and display an embedded virtual tour.",
  "main": "",
  "scripts": {
    "//[ SASS ***** ]//": "",
    "sass:testapp": "sass --style=compressed client/testapp/sass/*.scss test/testapp/static/testapp/css/testapp.css",
    "sass:admin": "sass --style=compressed client/wagtail/sass/*.scss wagtail_360/static/wagtail_360/admin/css/panorama_panel.css",
    "sass": "npm run sass:testapp && npm run sass:admin",
    "//[ SCRIPTS ***** ]//": "",
    "parseurl": "uglifyjs client/wagtail/js/parse_url.js -o wagtail_360/static/wagtail_360/admin/js/parse_url.min.js",
    "streetview": "uglifyjs client/wagtail/js/street_view.js -o wagtail_360/static/wagtail_360/admin/js/street_view.min.js",
    "panorama": "uglifyjs client/testapp/js/panorama.js -o test/testapp/static/testapp/js/panorama.min.js",
    "admin:js": "npm run parseurl && npm run streetview",
    "//[ BUILD ***** ]//": "",
    "serve": "browser-sync start --proxy 127.0.0.1:8000 --files \"test/testapp/templates/*.html\" \"test/testapp/static/testapp/css/*.css\" \"test/testapp/static/testapp/js/*.js\" \"wagtail_360/static/wagtail_360/admin/css/*.css\" \"wagtail_360/static/wagtail_360/admin/js/*.js\" \"wagtail_360/static/wagtail_360/client/js/*.js\"",
    "build": "npm run sass && npm run admin:js && npm run panorama",
    "start": "npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nickmoreton/wagtail-360.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nickmoreton/wagtail-360/issues"
  },
  "homepage": "https://github.com/nickmoreton/wagtail-360#readme",
  "devDependencies": {
    "browser-sync": "^2.27.5",
    "sass": "^1.55.0",
    "uglify-js": "^3.17.3"
  }
}
